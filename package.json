  "description": "A simple Chrome extension to help you use your time more effectively by showing you how much time do you have.",
  "main": "main.js",
  "engines": {
    "node": ">=9.0.0",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  },
  "scripts": {
    "clean": "rm -r ./dist/*",
    "build": "cross-env NODE_ENV=production npm run webpack",
    "start": "cross-env NODE_ENV=developmet npm run watch",
    "test": "cross-env NODE_ENV=test eslint ./src/**/*.js",
    "watch": "webpack --watch"
