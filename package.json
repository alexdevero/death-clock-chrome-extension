  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexdevero/death-clock-chrome-extension/issues"
  },
  "homepage": "https://github.com/alexdevero/death-clock-chrome-extension#readme",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "copy-webpack-plugin": "^4.3.1",
    "stylelint": "^8.4.0",
    "webpack": "^3.10.0"
